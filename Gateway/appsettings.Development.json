{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "write-route": {
        "ClusterId": "write-cluster",
        "Match": {
          "Path": "write/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catchall}"
          }
        ]
      },
      "read-route": {
        "ClusterId": "read-cluster",
        "Match": {
          "Path": "read/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catchall}"
          }
        ]
      }
    },
    "Clusters": {
      "write-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:8000/"
          }
        }
      },
      "read-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:8001/"
          }
        }
      }
    }
  },
  "IdentityServer": {
    "Uri": "https://localhost:8003"
  },
  "AllowedOrigins": {
    "Uris": "https://localhost:44485"
  }
}
